{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "Mint Starter Kit",
  "colors": {
    "primary": "#0e0e0e",
    "light": "#fdfdf7",
    "dark": "#09090b"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Welcome",
        "pages": ["index"]
      },
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Getting started",
            "pages": ["quickstart", "development"]
          },
          {
            "group": "Customization",
            "pages": ["essentials/settings", "essentials/navigation"]
          },
          {
            "group": "Writing content",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/reusable-snippets"
            ]
          },
          {
            "group": "AI tools",
            "pages": [
              "ai-tools/cursor",
              "ai-tools/claude-code",
              "ai-tools/windsurf"
            ]
          }
        ]
      },
      {
        "tab": "API reference",
        "groups": [
          {
            "group": "API documentation",
            "pages": ["api-reference/introduction"]
          },
          {
            "group": "Endpoint examples",
            "pages": [
              "api-reference/endpoint/get",
              "api-reference/endpoint/create",
              "api-reference/endpoint/delete",
              "api-reference/endpoint/webhook"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Playground",
          "href": "https://fish.audio",
          "icon": "play"
        },
        {
          "anchor": "Blog",
          "href": "https://openaudio.com",
          "icon": "newspaper"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@fish.audio"
      },
      {
        "label": "Login",
        "href": "https://fish.audio/auth"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Sign Up",
      "href": "https://fish.audio/auth/signup"
    }
  },
  "contextual": {
    "options": [
      "copy",
      "view",
      "chatgpt",
      "claude",
      "perplexity",
      "mcp",
      "cursor",
      "vscode"
    ]
  },
  "fonts": {
    "family": "Onest"
  },
  "footer": {
    "socials": {
      "github": "https://github.com/fishaudio",
      "x": "https://x.com/fishaudio",
      "linkedin": "https://www.linkedin.com/company/open-audio/",
      "discord": "https://discord.gg/dF9Db2Tt3Y"
    }
  }
}
